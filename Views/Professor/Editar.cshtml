@{
    var professor = ViewBag.professorRetornado;
}<h1> Página de Cadastro de Professores</h1>
<form action='@Url.Action("Atualizar")' method="post" enctype="multipart/form-data" class="form-group">
    <div class="form-professor-id">
        <label for="id">ID:</label>
        <input type="text" name="professorId" id="id" value="@professor.Id" class="form-control"/>
    </div>
    <div>
        <label for="nome">Nome:</label>
        <input type="text" name="professorNome" placeholder="Ex. Fulano de Tal" id="nome" value="@professor.Nome" class="form-control"/>
    </div>
    <div>
        <label for="email">Email:</label>
        <input type="text" name="professorEmail" placeholder="Ex. fulano@email.com" id="email"value="@professor.Email" class="form-control"/>
    </div>
    <div>
        <label for="end">Endereço:</label>
        <input type="text" name="professorEnd" placeholder="Ex. Rua 123" id="end"value="@professor.Endereco" class="form-control"/>
    </div>
    <div>
        <label for="tel">Telefone:</label>
        <input type="text" name="professorTel" placeholder="Ex. 11 98989-9898" id="tel"value="@professor.Telefone" class="form-control"/>
    </div>
    <div>
        <label for="esc">Cargo:</label>
        <input type="text" name="professorCargo" placeholder="Ex. Nível I" id="esc"value="@professor.Cargo" class="form-control"/>
    </div>
    <button type="submit">Atualizar</button>
</form>